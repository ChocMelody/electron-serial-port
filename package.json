{
  "name": "electron-serial-port-http",
  "version": "1.0.0",
  "description": "Electron application to forward serial port data to HTTP endpoint",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -w serialport -p -t \"cpu\" --version=21.4.4",
    "build:win": "npm run build && electron-builder --win --config",
    "pack": "electron-vite build && electron-builder --dir --config"
  },
  "keywords": [
    "electron",
    "serialport",
    "http",
    "vue3",
    "nestjs"
  ],
  "author": "Developer",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^11.1.8",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^11.1.8",
    "@nestjs/websockets": "^11.1.8",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "axios": "^1.2.0",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "electron-store": "^8.1.0",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "kafkajs": "^2.2.4",
    "mqtt": "^5.14.1",
    "nats": "^2.29.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.0",
    "serialport": "^13.0.0",
    "vue-virtual-scroll-list": "^2.3.5"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "electron": "^21.0.0",
    "electron-builder": "^23.6.0",
    "electron-prebuilt": "^1.3.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^1.0.27",
    "element-plus": "^2.2.17",
    "pinia": "^2.0.23",
    "typescript": "^4.8.4",
    "vite": "^4.5.14",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  }
}
